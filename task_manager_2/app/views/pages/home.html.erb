<% if current_user %>

	<% if current_user.access == 1 %>

		<%= render partial: "super_manager_dash" %>

	<% elsif current_user.access == 2 %>

		<%= render partial: "manager_dash" %>

	<% else %>

		<%= render partial: "staffer_dash" %>		

	<% end %>

<% else %>
	
	<%= render :partial => "sessions/new" %>

<% end %>
